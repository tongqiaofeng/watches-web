.vue-directive-image-previewer{position:fixed;top:0;left:0;width:100%;height:100%}.vue-directive-image-previewer[disabled]{pointer-events:none}.vue-directive-image-previewer-background{position:absolute;width:100%;height:100%;background-color:rgba(0,0,0,.5);opacity:0}.vue-directive-image-previewer-body{width:100%;height:100%;z-index:1}.vue-directive-image-previewer-body img{position:absolute;z-index:1}