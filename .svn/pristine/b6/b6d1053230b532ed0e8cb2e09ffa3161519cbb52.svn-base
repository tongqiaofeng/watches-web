A,GAAA,CAAA,eAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,UAAA,KAAA,CAAA,cAAA,GAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,UAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,UAAA;;AACA,cAAA,KAAA,CAAA,cAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,YAAA,KAAA,CAAA,OAAA,GAAA,SAAA;AACA;AACA,SAhBA;AAiBA,OAtBA,MAsBA,IAAA,KAAA,OAAA,IAAA,EAAA,EAAA;AACA,aAAA,IAAA,GAAA,CAAA;AACA,aAAA,cAAA,GAAA,EAAA;AACA,aAAA,KAAA,GAAA,CAAA;AACA,aAAA,QAAA,GAAA,CAAA;AACA,aAAA,iBAAA;AACA;AACA,KAhCA;AAiCA;AACA,IAAA,iBAlCA,+BAkCA;AAAA;;AACA,WAAA,OAAA,GAAA,UAAA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,iBAAA,EAAA;AACA,QAAA,IAAA,EAAA,KAAA,IADA;AAEA,QAAA,OAAA,EAAA,KAAA,OAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,SAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,QAAA,MAAA,CAAA,cAAA,GAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,UAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,UAAA;;AACA,YAAA,MAAA,CAAA,cAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,SAAA;AACA;AACA,OAbA,EAaA,KAbA,CAaA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAfA;AAgBA,KApDA;AAqDA;AACA,IAAA,gBAtDA,4BAsDA,GAtDA,EAsDA;AACA,MAAA,OAAA,CAAA,GAAA,wBAAA,GAAA;AACA,KAxDA;AAyDA,IAAA,mBAzDA,+BAyDA,GAzDA,EAyDA;AACA,MAAA,OAAA,CAAA,GAAA,+BAAA,GAAA;AACA,WAAA,IAAA,GAAA,GAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,IAAA;;AACA,UAAA,KAAA,OAAA,KAAA,EAAA,EAAA;AACA,aAAA,aAAA;AACA,OAFA,MAEA,IAAA,KAAA,OAAA,IAAA,EAAA,EAAA;AACA,aAAA,iBAAA;AACA;;AACA,OAAA,SAAA,YAAA,GAAA;AACA,YAAA,aAAA,GACA,QAAA,CAAA,eAAA,CAAA,SAAA,IAAA,QAAA,CAAA,IAAA,CAAA,SADA;;AAEA,YAAA,aAAA,GAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,qBAAA,CAAA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,CAAA,EAAA,aAAA,GAAA,aAAA,GAAA,CAAA;AACA;AACA,OAPA;AAQA;AA1EA;AAnBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"shipping-order-container\">\r\n    <!-- <h3>已入库/入库待售</h3> -->\r\n    <div v-show=\"forAccountList.length == 0\" ref=\"hello\" style=\"text-align: center;\">\r\n      <p>{{hintMsg}}</p>\r\n    </div>\r\n    <div v-if=\"forAccountList.length !== 0\">\r\n      <div style=\"margin-left: 20px;\">\r\n        <p style=\"font-size: 18px;\">{{'商品数量:  ' + ' ' + totalNum}}</p>\r\n      </div>\r\n      <div class=\"table-container\">\r\n        <table>\r\n          <tr>\r\n            <th>图片</th>\r\n            <th>型号</th>\r\n            <th>机芯号</th>\r\n            <th>采购价</th>\r\n            <th>入库时间</th>\r\n          </tr>\r\n        </table>\r\n        <div v-for=\"(items,index) of forAccountList\" :key=\"index\">\r\n          <table>\r\n            <tr v-for=\"(item,index) of items.watch\" :key=\"index\" style=\"background-color: #f3fbf9;\">\r\n              <td>\r\n                <img v-image-preview\r\n                  :src=\"item.buy_watchPics == null || item.buy_watchPics == '' ? '' : img + '/img/watch/'+ item.buy_watchPics.split('|')[0]\"\r\n                  class=\"first-img\" />\r\n              </td>\r\n              <td>\r\n                <p>{{item.buy_watchBrand}}</p>\r\n                <p>{{item.buy_watchModel}}</p>\r\n              </td>\r\n              <td>{{item.buy_watchSn}}</td>\r\n              <td>{{item.buy_watchCurrency + ' ' + formatNumberRgx(item.buy_watchPrice)}}</td>\r\n              <td>{{item.stock_inTime}}</td>\r\n            </tr>\r\n          </table>\r\n        </div>\r\n      </div>\r\n      <div style=\"width: 100%;height: 63px;\">\r\n        <div style=\"margin:15px 0;position:absolute;right:6%;\">\r\n          <el-pagination @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\" :current-page=\"page\"\r\n            layout=\"total, prev, pager, next, jumper\" :total=\"total\"></el-pagination>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        hintMsg: '数据加载中...',\r\n        page: 1,\r\n        pageNum: 10,\r\n        keyword: '',\r\n        total: 0,\r\n        totalNum: 0,\r\n        forAccountList: [],\r\n        img: this.$store.state.baseUrl,\r\n        dialogPurchaseVisible: false,\r\n        purchaseOrder: [],\r\n\r\n      }\r\n    },\r\n    created() {\r\n      this.getForAccountList();\r\n    },\r\n    methods: {\r\n      // 模糊搜索\r\n      stockInSearch() {\r\n        if (this.keyword !== '') {\r\n          this.forAccountList = [];\r\n          this.total = 0;\r\n          this.totalNum = 0;\r\n          this.hintMsg = '数据加载中...';\r\n          this.$axios\r\n            .post(this.$store.state.baseUrl + \"/BuyOrderListEx\", {\r\n              page: this.page,\r\n              pageNum: this.pageNum,\r\n              type: 3,\r\n              keyword: this.keyword\r\n            })\r\n            .then(res => {\r\n              console.log(\"模糊搜索获取已入库商品列表\");\r\n              console.log(res);\r\n              this.forAccountList = res.data.orders;\r\n              this.total = res.data.watchTotal;\r\n              this.totalNum = res.data.watchTotal;\r\n              if (this.forAccountList.length == 0) {\r\n                this.hintMsg = '啊哦~暂无数据'\r\n              }\r\n            });\r\n        } else if (this.keyword == '') {\r\n          this.page = 1;\r\n          this.forAccountList = [];\r\n          this.total = 0;\r\n          this.totalNum = 0;\r\n          this.getForAccountList();\r\n        }\r\n      },\r\n      // 获取待结算商品\r\n      getForAccountList() {\r\n        this.hintMsg = '数据加载中...';\r\n        this.$axios.post(this.$store.state.baseUrl + '/BuyOrderListEx', {\r\n          page: this.page,\r\n          pageNum: this.pageNum,\r\n          type: 3\r\n        }).then((res) => {\r\n          console.log('已入库商品列表');\r\n          console.log(res);\r\n          this.forAccountList = res.data.orders;\r\n          this.total = res.data.watchTotal;\r\n          this.totalNum = res.data.watchTotal;\r\n          if (this.forAccountList.length == 0) {\r\n            this.hintMsg = '啊哦~暂无数据'\r\n          }\r\n        }).catch((err) => {\r\n          console.log(err);\r\n        })\r\n      },\r\n      // 分页\r\n      handleSizeChange(val) {\r\n        console.log(`每页 ${val} 条`);\r\n      },\r\n      handleCurrentChange(val) {\r\n        console.log(`当前页: ${val}`);\r\n        this.page = val;\r\n        console.log(this.page);\r\n        if (this.keyword !== '') {\r\n          this.stockInSearch();\r\n        } else if (this.keyword == '') {\r\n          this.getForAccountList();\r\n        }\r\n        (function smoothscroll() {\r\n          var currentScroll =\r\n            document.documentElement.scrollTop || document.body.scrollTop;\r\n          if (currentScroll > 0) {\r\n            window.requestAnimationFrame(smoothscroll);\r\n            window.scrollTo(0, currentScroll - currentScroll / 5);\r\n          }\r\n        })();\r\n      },\r\n    },\r\n  }\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n  .shipping-order-container {\r\n    width: 100%;\r\n    margin: 0 auto;\r\n    margin-top: 75px;\r\n\r\n    .table-container {\r\n      padding: 30px;\r\n      background-color: #fff;\r\n      border-radius: 30px;\r\n\r\n      td {\r\n        padding: 20px 0;\r\n\r\n        .first-img {\r\n          width: 100px;\r\n          height: 100px;\r\n          object-fit: cover;\r\n          border-radius: 30px;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  table {\r\n    width: 100%;\r\n    table-layout: fixed;\r\n    border-collapse: separate;\r\n    border-spacing: 0;\r\n\r\n    tr {\r\n\r\n      th,\r\n      td {\r\n        width: 21%;\r\n        text-align: center;\r\n        border: 0;\r\n      }\r\n    }\r\n  }\r\n</style>"],"sourceRoot":"src/components/purch"}]}