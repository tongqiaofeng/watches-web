<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
  <link rel="icon" href="<%= BASE_URL %>toptimeclub-01.png">
  <title>TopTime在线办公系统</title>
  <style>
    .el-message {
      top: 100px !important;
    }
  </style>
</head>

<body>
  <div id="app"></div>
  <script>
    console.log('比例');
    console.log(window.devicePixelRatio);
    let userAgent = navigator.userAgent; // 取得浏览器的userAgent字符串
    console.log(userAgent);
    let isChrome = navigator.userAgent.indexOf('Chrome') > -1; // 是否是谷歌 
    console.log('谷歌' + isChrome); // 返回 true  则是 ，false  则不是
    if (isChrome) {
      if (window.devicePixelRatio > 1) {
        console.log(1 / window.devicePixelRatio);
        let s = Number(1 / window.devicePixelRatio);
        document.body.style.zoom = s;
      } else {
        document.body.style.zoom = 1;
      };
    }
  </script>
  <script type="text/javascript"
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB-VK6HVEWAsnknuEWEVWTDN6Uwg2e23kc&sensor"></script>
</body>

</html>