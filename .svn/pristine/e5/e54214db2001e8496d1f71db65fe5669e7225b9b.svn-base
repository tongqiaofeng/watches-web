,MAAA,CAAA,KAAA,CAAA,OAAA;;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,OAAA;;AACA,QAAA,MAAA,CAAA,QAAA;;AACA,QAAA,MAAA,CAAA,uBAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,UAAA;AACA,OA1BA,EA0BA,KA1BA,CA0BA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OA5BA;AA6BA,KArIA;AAsIA;AACA,IAAA,UAvIA,wBAuIA;AACA,WAAA,SAAA,GAAA,EAAA;;AADA,kDAEA,KAAA,WAFA;AAAA;;AAAA;AAEA,+DAAA;AAAA,cAAA,KAAA;AACA,eAAA,SAAA,CAAA,IAAA,CAAA,KAAA,CAAA,KAAA;AACA;AAJA;AAAA;AAAA;AAAA;AAAA;;AAKA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,SAAA;AACA,WAAA,SAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,CAAA,GAAA,CAAA;AACA,OAFA;AAGA;AAhJA;AAzCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- <h3>批发价详情页面</h3> -->\r\n    <div class=\"details-center\">\r\n      <div class=\"back-img\">\r\n        <img src=\"../../assets/imgs/goback.png\" @click=\"gobackAdmin\" />\r\n        <span class=\"font\">返回</span>\r\n      </div>\r\n    </div>\r\n    <div v-show=\"detailsList.length > 1\" id=\"myChart\" style=\"width: 80%;height: 400px;margin: 30px auto;\"></div>\r\n    <div style=\"width: 50%;margin: 30px auto;display: flex;\">\r\n      <el-input v-model=\"price\" placeholder=\"请输入批发价格\"></el-input>\r\n      <el-button type=\"primary\" @click=\"priceAdd\" style=\"width: 30%;\">增加</el-button>\r\n    </div>\r\n    <div v-if=\"detailsList.length == 0\" style=\"text-align: center;\">\r\n      <p>暂无价格列表</p>\r\n    </div>\r\n    <div class=\"details-bottom\" v-if=\"detailsList.length !== 0\">\r\n      <div style=\"display: flex;justify-content: space-around;\">\r\n        <div>\r\n          <span>最高：</span>\r\n          <span>{{'HKD ' + formatNumberRgx(priceList[priceList.length - 1])}}</span>\r\n        </div>\r\n        <div>\r\n          <span>最低：</span>\r\n          <span>{{'HKD ' + formatNumberRgx(priceList[0])}}</span>\r\n        </div>\r\n      </div>\r\n      <table>\r\n        <tr>\r\n          <th>价格</th>\r\n          <th>趋势</th>\r\n          <th>修改时间</th>\r\n          <th>操作</th>\r\n        </tr>\r\n        <tr v-for=\"(item,index) in detailsList\" :key=\"index\">\r\n          <td class=\"first-td\">{{\"HKD \" + formatNumberRgx(item.price)}}</td>\r\n          <td>\r\n            <!-- 数组最后一项不显示此内容，该判断条件也决定了数组的长度需大于1 -->\r\n            <div v-if=\"index < detailsList.length-1\">\r\n              <img :src=\"tendency2(index)\" style=\"width: 25px;height: 25px;\" />\r\n            </div>\r\n          </td>\r\n          <td>{{item.time}}</td>\r\n          <td class=\"last-td\">\r\n            <p class=\"font-style\" @click=\"deletPrice(item.id)\">删除</p>\r\n            <el-dialog title=\"删除批发价格\" :visible.sync=\"dialogDeletPriceVisible\">\r\n              <div style=\"text-align: center;\">\r\n                <p>确定删除该批发价格？删除后不可恢复</p>\r\n              </div>\r\n              <div slot=\"footer\">\r\n                <el-button @click=\"dialogDeletPriceVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"deletPriceSure\">确 定</el-button>\r\n              </div>\r\n            </el-dialog>\r\n          </td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n  let that;\r\n  export default {\r\n    data() {\r\n      return {\r\n        price: '',\r\n        watchId: 0,\r\n        detailsList: [],\r\n        dialogDeletPriceVisible: false,\r\n        priceId: 0,\r\n        priceList: [],\r\n        highest: 0,\r\n        lowest: 0,\r\n        tendencyImg: '',\r\n        img1: require('../../assets/imgs/upPrice .png'),\r\n        img2: require('../../assets/imgs/downPrice .png'),\r\n        xList: [],\r\n        yList: [],\r\n        dataMaketPriceCount: 0,\r\n\r\n      }\r\n    },\r\n    props: [\"priceDetailsList\"],\r\n    created() {\r\n      console.log(\"批发价详情页面\");\r\n      console.log(this.priceDetailsList);\r\n      this.watchId = this.priceDetailsList.buy_watchid;\r\n      this.detailsList = this.priceDetailsList.prices;\r\n      this.xList = [];\r\n      this.yList = [];\r\n      if (this.detailsList.length > 1) {\r\n        for (let item of this.detailsList) {\r\n          this.xList.push(item.time);\r\n          this.yList.push(item.price);\r\n        }\r\n      }\r\n      this.xList.reverse();\r\n      this.yList.reverse();\r\n      this.judgePrice();\r\n    },\r\n    mounted() {\r\n      this.drawLine();\r\n    },\r\n    methods: {\r\n      tendency2(i) {\r\n        console.log('比较大小');\r\n        if (this.detailsList[i].price > this.detailsList[i + 1].price) {\r\n          return require('../../assets/imgs/upPrice .png');\r\n        } else {\r\n          return require('../../assets/imgs/downPrice .png');\r\n        }\r\n      },\r\n      // 返回价格列表\r\n      gobackAdmin() {\r\n        this.$emit('gobackPriceAdmin', 0);\r\n      },\r\n      // 需录入数量\r\n      priceCount() {\r\n        this.$axios.post(this.$store.state.baseUrl + \"/DataMaketPriceCount\").then((res) => {\r\n          console.log('批发价总数');\r\n          console.log(res);\r\n          this.dataMaketPriceCount = res.data.total;\r\n          sessionStorage.setItem('maketPriceCount', this.dataMaketPriceCount);\r\n        })\r\n      },\r\n      // 增加批发价格\r\n      priceAdd() {\r\n        if (this.price == '') {\r\n          this.$message.error({\r\n            message: '请输入批发价格',\r\n            showClose: true,\r\n            duration: 2000\r\n          })\r\n        } else {\r\n          this.$axios.post(this.$store.state.baseUrl + '/DataMaketPriceAdd', {\r\n            id: this.watchId,\r\n            price: this.price\r\n          }).then((res) => {\r\n            console.log('增加批发价格');\r\n            console.log(res);\r\n            this.$message.success({\r\n              message: '增加批发价格成功',\r\n              showClose: true,\r\n              duration: 2000\r\n            })\r\n            this.detailsList = res.data;\r\n            this.judgePrice();\r\n            this.xList = [];\r\n            this.yList = [];\r\n            if (this.detailsList.length > 1) {\r\n              for (let item of this.detailsList) {\r\n                this.xList.push(item.time);\r\n                this.yList.push(item.price);\r\n              }\r\n            }\r\n            this.xList.reverse();\r\n            this.yList.reverse();\r\n            this.drawLine();\r\n            this.price = '';\r\n            this.priceCount();\r\n          }).catch((err) => {\r\n            console.log(err);\r\n          })\r\n        }\r\n      },\r\n      // 绘制折线图\r\n      drawLine() {\r\n        // Vue 实现响应式并不是数据发生变化之后 DOM 立即变化，nextTick会保证其内的代码在DOM更新后执行\r\n        this.$nextTick(() => {\r\n          // 基于准备好的dom，初始化echarts实例\r\n          let myChart = this.$echarts.init(document.getElementById('myChart'));\r\n          myChart.resize();\r\n          // 绘制图表\r\n          myChart.setOption({\r\n            title: {\r\n              text: '批发价折线统计图',\r\n              x: 'center'\r\n            },\r\n            grid: {\r\n              width: '100%',\r\n              height: 'auto'\r\n            },\r\n            tooltip: {},\r\n            xAxis: {\r\n              data: this.xList\r\n            },\r\n            yAxis: {},\r\n            series: [{\r\n              name: '批发价',\r\n              type: 'line',\r\n              data: this.yList\r\n            }]\r\n          });\r\n          window.addEventListener(\"resize\", function () {\r\n            myChart.resize();\r\n          })\r\n        })\r\n\r\n      },\r\n      // 删除批发价格\r\n      deletPrice(id) {\r\n        console.log(id);\r\n        this.priceId = id;\r\n        this.dialogDeletPriceVisible = true;\r\n      },\r\n      // 确定删除\r\n      deletPriceSure() {\r\n        this.$axios.post(this.$store.state.baseUrl + '/DataMaketPriceDel', {\r\n          id: this.priceId,\r\n          watchid: this.watchId\r\n        }).then((res) => {\r\n          console.log('删除价格');\r\n          console.log(res);\r\n          this.detailsList = res.data;\r\n          this.$message.success({\r\n            message: '删除该批发价格成功',\r\n            showClose: true,\r\n            duration: 2000\r\n          });\r\n          this.judgePrice();\r\n          this.xList = [];\r\n          this.yList = [];\r\n          if (this.detailsList.length > 1) {\r\n            for (let item of this.detailsList) {\r\n              this.xList.push(item.time);\r\n              this.yList.push(item.price);\r\n            }\r\n          }\r\n          this.xList.reverse();\r\n          this.yList.reverse();\r\n          this.drawLine();\r\n          this.dialogDeletPriceVisible = false;\r\n          this.priceCount();\r\n        }).catch((err) => {\r\n          console.log(err);\r\n        })\r\n      },\r\n      // 判断最高最低价格\r\n      judgePrice() {\r\n        this.priceList = [];\r\n        for (let price of this.detailsList) {\r\n          this.priceList.push(price.price);\r\n        }\r\n        console.log(this.priceList);\r\n        this.priceList.sort((a, b) => {\r\n          return a - b;\r\n        })\r\n      },\r\n    },\r\n  }\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n  .details-center {\r\n    width: 80%;\r\n    margin: 0 auto;\r\n    // display: flex;\r\n    // justify-content: space-between;\r\n\r\n    .back-img {\r\n      height: 65px;\r\n      line-height: 65px;\r\n      display: flex;\r\n      cursor: pointer;\r\n\r\n      .font {\r\n        font-size: 30px;\r\n        color: #2d4e65;\r\n      }\r\n    }\r\n\r\n  }\r\n\r\n  .details-bottom {\r\n    width: 80%;\r\n    margin: 0 auto;\r\n    padding: 20px;\r\n    background-color: #fff;\r\n    border-radius: 30px;\r\n\r\n    td {\r\n      height: 60px;\r\n      margin: 10px 0;\r\n      padding: 20px 0;\r\n      background-color: #f2f5f7;\r\n      font-size: 17px;\r\n    }\r\n\r\n    .first-td {\r\n      border-top-left-radius: 30px;\r\n      border-bottom-left-radius: 30px;\r\n    }\r\n\r\n    .last-td {\r\n      border-top-right-radius: 30px;\r\n      border-bottom-right-radius: 30px;\r\n\r\n      .font-style {\r\n        margin: 0;\r\n        margin-top: 10px;\r\n        color: #0aa1ed;\r\n        font-size: 15px;\r\n        cursor: pointer;\r\n      }\r\n    }\r\n  }\r\n\r\n  table {\r\n    width: 100%;\r\n    table-layout: fixed;\r\n    border-collapse: separate;\r\n    border-spacing: 0 30px;\r\n\r\n    tr {\r\n\r\n      th,\r\n      td {\r\n        width: 20%;\r\n        text-align: center;\r\n        border: 0;\r\n      }\r\n    }\r\n  }\r\n</style>"],"sourceRoot":"src/components/views"}]}