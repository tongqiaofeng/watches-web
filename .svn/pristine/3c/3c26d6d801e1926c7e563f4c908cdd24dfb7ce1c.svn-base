,IAAA,CAAA,IAAA,CAAA;AACA,eAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA;AAJA;AAAA;AAAA;AAAA;AAAA;AAKA;;AACA,SAAA,KAAA,CAAA,OAAA;AACA,SAAA,KAAA,CAAA,OAAA;AACA,SAAA,UAAA;AACA,GAjCA;AAkCA,EAAA,OAlCA,qBAkCA;AACA,SAAA,QAAA;AACA,GApCA;AAqCA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,oBACA,CADA,EACA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EADA,CAEA;;AACA,UAAA,KAAA,WAAA,CAAA,CAAA,EAAA,KAAA,GAAA,KAAA,WAAA,CAAA,CAAA,GAAA,CAAA,EAAA,KAAA,EAAA;AACA,eAAA,OAAA,CAAA,+BAAA,CAAA;AACA,OAFA,MAEA;AACA,eAAA,OAAA,CAAA,iCAAA,CAAA;AACA;AACA,KATA;AAUA;AACA,IAAA,QAXA,sBAWA;AAAA;;AACA;AACA,WAAA,SAAA,CAAA,YAAA;AACA;AACA,YAAA,OAAA,GAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAAA,cAAA,CAAA,SAAA,CAAA,CAAA;;AACA,QAAA,OAAA,CAAA,MAAA,GAHA,CAIA;;AACA,QAAA,OAAA,CAAA,SAAA,CAAA;AACA,UAAA,KAAA,EAAA;AACA,YAAA,IAAA,EAAA,UADA;AAEA,YAAA,CAAA,EAAA;AAFA,WADA;AAKA,UAAA,OAAA,EAAA,EALA;AAMA,UAAA,KAAA,EAAA;AACA,YAAA,IAAA,EAAA,KAAA,CAAA;AADA,WANA;AASA,UAAA,KAAA,EAAA,EATA;AAUA,UAAA,MAAA,EAAA,CAAA;AACA,YAAA,IAAA,EAAA,KADA;AAEA,YAAA,IAAA,EAAA,MAFA;AAGA,YAAA,IAAA,EAAA,KAAA,CAAA;AAHA,WAAA;AAVA,SAAA;AAgBA,QAAA,MAAA,CAAA,gBAAA,CAAA,QAAA,EAAA,YAAA;AACA,UAAA,OAAA,CAAA,MAAA;AACA,SAFA;AAGA,OAxBA;AA0BA,KAvCA;AAwCA;AACA,IAAA,WAzCA,yBAyCA;AACA,WAAA,KAAA,CAAA,kBAAA,EAAA,CAAA;AACA,KA3CA;AA4CA;AACA,IAAA,UA7CA,wBA6CA;AACA,WAAA,SAAA,GAAA,EAAA;;AADA,kDAEA,KAAA,WAFA;AAAA;;AAAA;AAEA,+DAAA;AAAA,cAAA,KAAA;AACA,eAAA,SAAA,CAAA,IAAA,CAAA,KAAA,CAAA,KAAA;AACA;AAJA;AAAA;AAAA;AAAA;AAAA;;AAKA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,SAAA;AACA,WAAA,SAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,CAAA,GAAA,CAAA;AACA,OAFA;AAGA;AAtDA;AArCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- <h3>批发价详情页面</h3> -->\r\n    <div class=\"details-center\">\r\n      <div class=\"back-img\" @click=\"gobackAdmin\">\r\n        <div>\r\n          <img src=\"../../assets/imgs/goback.png\" />\r\n        </div>\r\n        <span class=\"font\">返回</span>\r\n      </div>\r\n    </div>\r\n    <div v-show=\"detailsList.length > 1\" id=\"myChart\" style=\"width: 80%;height: 400px;margin: 30px auto;\"></div>\r\n    <div v-if=\"detailsList.length == 0\" style=\"text-align: center;\">\r\n      <p>暂无价格列表</p>\r\n    </div>\r\n    <div class=\"details-bottom\" v-if=\"detailsList.length !== 0\">\r\n      <div style=\"display: flex;justify-content: space-around;margin-bottom: 20px;\">\r\n        <div>\r\n          <span>最高：</span>\r\n          <span>{{'HKD ' + formatNumberRgx(priceList[priceList.length - 1])}}</span>\r\n        </div>\r\n        <div>\r\n          <span>最低：</span>\r\n          <span>{{'HKD ' + formatNumberRgx(priceList[0])}}</span>\r\n        </div>\r\n      </div>\r\n      <table>\r\n        <tr>\r\n          <th>价格</th>\r\n          <th>趋势</th>\r\n          <th>更新时间</th>\r\n        </tr>\r\n        <tr v-for=\"(item,index) in detailsList\" :key=\"index\">\r\n          <td>{{\"HKD \" + formatNumberRgx(item.price)}}</td>\r\n          <td>\r\n            <!-- 数组最后一项不显示此内容 -->\r\n            <div v-if=\"index < detailsList.length-1\">\r\n              <img :src=\"tendency(index)\" style=\"width: 25px;height: 25px;\" />\r\n            </div>\r\n          </td>\r\n          <td>{{item.time}}</td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n  let that;\r\n  export default {\r\n    data() {\r\n      return {\r\n        price: '',\r\n        detailsList: [],\r\n        dialogDeletPriceVisible: false,\r\n        priceId: 0,\r\n        priceList: [],\r\n        highest: 0,\r\n        lowest: 0,\r\n        tendencyImg: '',\r\n        xList: [],\r\n        yList: [],\r\n\r\n      }\r\n    },\r\n    props: [\"priceDetailsList\"],\r\n    created() {\r\n      console.log(\"批发价详情页面\");\r\n      console.log(this.priceDetailsList);\r\n      this.detailsList = this.priceDetailsList.prices;\r\n      console.log(this.detailsList);\r\n      this.xList = [];\r\n      this.yList = [];\r\n      if (this.detailsList.length > 1) {\r\n        for (let item of this.detailsList) {\r\n          this.xList.push(/\\d{4}-\\d{1,2}-\\d{1,2}/g.exec(item.time));\r\n          this.yList.push(item.price);\r\n        }\r\n      }\r\n      this.xList.reverse();\r\n      this.yList.reverse();\r\n      this.judgePrice();\r\n    },\r\n    mounted() {\r\n      this.drawLine();\r\n    },\r\n    methods: {\r\n      tendency(i) {\r\n        console.log('比较大小');\r\n        // 数组有一条以上数据并且不是数组最后一项才执行此操作\r\n        if (this.detailsList[i].price > this.detailsList[i + 1].price) {\r\n          return require('../../assets/imgs/upPrice.png');\r\n        } else {\r\n          return require('../../assets/imgs/downPrice.png');\r\n        }\r\n      },\r\n      // 绘制折线图\r\n      drawLine() {\r\n        // Vue 实现响应式并不是数据发生变化之后 DOM 立即变化，nextTick会保证其内的代码在DOM更新后执行\r\n        this.$nextTick(() => {\r\n          // 基于准备好的dom，初始化echarts实例\r\n          let myChart = this.$echarts.init(document.getElementById('myChart'));\r\n          myChart.resize();\r\n          // 绘制图表\r\n          myChart.setOption({\r\n            title: {\r\n              text: '批发价折线统计图',\r\n              x: 'center'\r\n            },\r\n            tooltip: {},\r\n            xAxis: {\r\n              data: this.xList\r\n            },\r\n            yAxis: {},\r\n            series: [{\r\n              name: '批发价',\r\n              type: 'line',\r\n              data: this.yList\r\n            }]\r\n          });\r\n          window.addEventListener(\"resize\", function () {\r\n            myChart.resize();\r\n          })\r\n        })\r\n\r\n      },\r\n      // 返回价格列表\r\n      gobackAdmin() {\r\n        this.$emit('gobackPriceAdmin', 0);\r\n      },\r\n      // 判断最高最低价格\r\n      judgePrice() {\r\n        this.priceList = [];\r\n        for (let price of this.detailsList) {\r\n          this.priceList.push(price.price);\r\n        }\r\n        console.log(this.priceList);\r\n        this.priceList.sort((a, b) => {\r\n          return a - b;\r\n        })\r\n      },\r\n    },\r\n  }\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n  .details-center {\r\n    width: 95%;\r\n    margin: 30px auto;\r\n    margin-top: 0;\r\n\r\n    .back-img {\r\n      width: 75px;\r\n      height: 45px;\r\n      line-height: 45px;\r\n      display: flex;\r\n      justify-content: space-between;\r\n      cursor: pointer;\r\n\r\n      div {\r\n        margin-top: 5px;\r\n\r\n        img {\r\n          width: 30px;\r\n          height: 25px;\r\n        }\r\n      }\r\n\r\n      .font {\r\n        font-size: 17px;\r\n      }\r\n    }\r\n\r\n  }\r\n\r\n  .details-bottom {\r\n    width: 92%;\r\n    margin: 0 auto;\r\n    padding: 20px;\r\n    background-color: #fff;\r\n    border-radius: 30px;\r\n\r\n    td {\r\n      height: 60px;\r\n      margin: 10px 0;\r\n      padding: 20px 0;\r\n      background-color: #f3fbf9;\r\n      font-size: 15px;\r\n    }\r\n  }\r\n\r\n  table {\r\n    width: 100%;\r\n    table-layout: fixed;\r\n    border-collapse: separate;\r\n    border-spacing: 0;\r\n\r\n    tr {\r\n\r\n      th,\r\n      td {\r\n        width: 20%;\r\n        text-align: center;\r\n        border: 0;\r\n      }\r\n    }\r\n  }\r\n</style>"],"sourceRoot":"src/components/views"}]}